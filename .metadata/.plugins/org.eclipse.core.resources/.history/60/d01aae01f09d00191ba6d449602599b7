package net.oaster2000.sin3d;

import java.awt.event.KeyEvent;

import net.oaster2000.sin3d.input.InputHandler;

public class Game {
	
	public int time;
	
	public Game() {
		
	}
	
	public void tick(InputHandler input) {
		time++;
		boolean forward = input.key[KeyEvent.VK_W];
		boolean backward = input.key[KeyEvent.VK_W];
		boolean left = input.key[KeyEvent.VK_W];
		boolean right = input.key[KeyEvent.VK_W];
	}
}
